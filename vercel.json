{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "node",
  "functions": {
    "dist/vercel.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
